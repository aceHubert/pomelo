{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"],
    "jsx": "preserve",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "baseUrl": "."
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/admin/**/*.ts",
    "src/admin/**/*.tsx",
    "src/initialize/**/*.ts",
    "src/initialize/**/*.tsx"
  ]
}
