<% if (process.env.NODE_ENV === 'production') { %>
<link rel="stylesheet" href="<%= baseURL %>style/password.min.css" />
<%} else { %>
<link rel="stylesheet" href="<%= baseURL %>style/password.css" />
<% } %>
<%- primaryStyleVars %>
<div id="app"><%- content %> <%- include('partials/footer') %></div>
<%- include('partials/axios') %> <%- include('partials/crypto') %>
<script>
  var locales = {
    passwordInvalid:
      '<%= tv("password.modify.form.new_password_invalid", "Please input password(6-16 characters)!", i18nLang) %>',
    dismatchPassword:
      '<%= tv("password.modify.form.confirm_password_dismatch", "Confirm password is not match.", i18nLang) %>',
  };
</script>
<% if (process.env.NODE_ENV === 'production') { %>
<script src="<%= baseURL %>js/password.min.js"></script>
<%} else { %>
<script src="<%= baseURL %>js/password.js"></script>
<% } %>
<%- contentFor('title') %> <%= title %>
