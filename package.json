{
  "name": "pomelo",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "serve:web": "yarn workspace @ace-pomelo/web run serve",
    "start:servers": "concurrently \"npm:start:server:*\"",
    "start:server:infrastructure-service": "nest build infrastructure-service --builder=webpack --watch",
    "start:server:infrastructure-bff": "nest build infrastructure-bff --builder=webpack --watch",
    "start:identity-server": "yarn workspace @ace-pomelo/theme run build && concurrently --raw \"grunt --gruntfile ./servers/identity-server/Gruntfile.js dev\" \"nest build identity-server --builder=webpack --watch\"",
    "build:web": "yarn workspaces foreach -vtR --from '@ace-pomelo/web' run build",
    "build:web:oidc": "cross-env VUE_APP_AUTH_TYPE=OIDC yarn workspaces foreach -vtR --from '@ace-pomelo/web' run build",
    "build:servers": "rimraf dist && concurrently \"npm:build:server:*\"",
    "build:server:infrastructure-service": "node -p \"'export const version = ' + JSON.stringify(require('./package.json').version) + ';'\" > ./servers/infrastructure-service/src/version.ts && nest build infrastructure-service --builder=webpack --config nest-cli.build.json",
    "build:server:infrastructure-bff": "node -p \"'export const version = ' + JSON.stringify(require('./package.json').version) + ';'\" > ./servers/infrastructure-bff/src/version.ts && nest build infrastructure-bff --builder=webpack --config nest-cli.build.json",
    "build:identity-server": "grunt --gruntfile ./servers/identity-server/Gruntfile.js && node -p \"'export const version = ' + JSON.stringify(require('./package.json').version) + ';'\" > ./servers/identity-server/src/version.ts && nest build identity-server --builder=webpack --config nest-cli.build.json",
    "build:submodules": "grunt --verbose",
    "build:submodules:formily-antdv": "grunt build:formily-antdv --verbose",
    "build:submodules:formily-vant": "grunt build:formily-vant --verbose",
    "build:submodules:formily-portal-antdv": "grunt build:formily-portal-antdv --verbose",
    "build:submodules:formily-portal-vant": "grunt build:formily-portal-vant --verbose",
    "build:submodules:formily": "grunt build:formily --verbose",
    "build:submodules:formily-portal": "grunt build:formily-portal --verbose",
    "build:submodules:antdv": "grunt build:antdv --verbose",
    "build:submodules:vant": "grunt build:vant --verbose",
    "clean:submodules": "grunt clean:all --verbose",
    "test": "jest",
    "test:watch": "run -T jest --watchAll",
    "test:cov": "run -T jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "test:e2e:identity-server": "run -T jest --config ./servers/identity-server/__test__/jest-e2e.json",
    "test:e2e:infrastructure-service": "run -T jest --config ./infrastructure-service/__test__/jest-e2e.json",
    "test:e2e:infrastructure-bff": "run -T jest --config ./infrastructure-bff/__test__/jest-e2e.json",
    "lint": "yarn workspaces foreach -vpt run lint",
    "lint:fix": "yarn workspaces foreach -vpt run lint:fix",
    "lint:check": "eslint --ext .js,.jsx,.ts,.tsx,.vue --max-warnings 0 \"**/src/**\"",
    "prettier": "node ./scripts/prettier.js",
    "prettier:all": "node ./scripts/prettier.js write",
    "commit": "git-cz"
  },
